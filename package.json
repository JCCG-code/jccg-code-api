{
  "name": "jccg-code-api",
  "version": "1.0.0",
  "description": "JCCG Code Official API",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "docs": "apidoc -i . -o apidoc_output/",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "docker": "docker build -t juankycg/jccg-code-api:latest . && docker run -it --rm -p 4500:4500 --name jccg-code-api-container juankycg/jccg-code-api",
    "docker:deploy": "docker build -t juankycg/jccg-code-api:latest . && docker push juankycg/jccg-code-api:latest"
  },
  "keywords": [
    "api",
    "express",
    "esm"
  ],
  "author": "Tu Nombre",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@google/genai": "^1.5.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mime-types": "^3.0.1",
    "mongoose": "^8.15.1",
    "music-metadata": "^11.2.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "apidoc": "^1.2.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.7.1",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
